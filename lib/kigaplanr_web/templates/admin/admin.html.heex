<div class="phx-hero">
    <h2>Generate Teacher Accounts Here</h2>
</div>

<%= form_for @changeset, Routes.admin_path(@conn, :create), fn f -> %>
  <label>
    Name: <%= text_input f, :name %>
  </label>

  <label>
    Age: <%= select f, :age, 18..100 %>
  </label>

  <%= submit "Submit" %>
<% end %>